{
  "type": "module",
  "scripts": {
    "dev": "concurrently --raw \"npm:dev:*\"",
    "dev:astro": "astro dev",
    "dev:css": "chokidar \"src/styles/{base,components,utilities}.*.css\" --command \"touch tailwind.config.cjs\" --debounce 0 --silent",
    "build": "astro check && tsc --noEmit && astro build",
    "preview": "astro preview",
    "test": "vitest",
    "lint": "prettier . --check && eslint .",
    "lint:fix": "prettier . --write && eslint . --fix"
  },
  "dependencies": {
    "@astrojs/check": "0.9.6",
    "@astrojs/mdx": "4.3.12",
    "@astrojs/sitemap": "3.6.0",
    "@tailwindcss/vite": "4.1.18",
    "@tsparticles/engine": "3.9.1",
    "@types/imagesloaded": "4.1.5",
    "astro": "5.16.2",
    "microcms-js-sdk": "2.7.0",
    "patch-package": "8.0.0",
    "rollup-plugin-visualizer": "5.9.2",
    "sass": "1.72.0",
    "swiper": "12.0.3",
    "tailwindcss": "4.1.18",
    "tsparticles": "3.9.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "chokidar-cli": "3.0.0",
    "concurrently": "8.2.2",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-astro": "0.29.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-tailwindcss": "4.0.0-beta.0",
    "imagesloaded": "5.0.0",
    "jsdom": "22.1.0",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.1",
    "prettier-plugin-css-order": "2.0.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": "20"
  },
  "packageManager": "npm@10.2.2",
  "private": true,
  "volta": {
    "node": "20.19.0"
  },
  "overrides": {
    "vite": "6.0.2"
  }
}
